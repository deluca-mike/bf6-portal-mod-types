{
    "name": "bf6-portal-mod-types",
    "version": "1.1.3-docs.1",
    "description": "TypeScript definitions for Battlefield 6 Portal mod namespace",
    "engines": {
        "node": ">=23.0.0"
    },
    "main": "",
    "types": "index.d.ts",
    "files": [
        "index.d.ts",
        "enums.d.ts",
        "event-handler-signatures.d.ts",
        "types.d.ts",
        "enums/",
        "runtime-spawn-enums/",
        "README.md"
    ],
    "exports": {
        ".": {
            "types": "./index.d.ts"
        }
    },
    "scripts": {
        "prettier": "prettier --no-error-on-unmatched-pattern --write '**/*.{ts,js,mjs,md,json}'",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "build": "node ./src/merge-docs.js && npm run prettier",
        "verify": "node ./src/verify-build.js",
        "docs": "typedoc --commentStyle all"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/deluca-mike/bf6-portal-mod-types.git"
    },
    "keywords": [
        "battlefield",
        "battlefield-6",
        "portal",
        "typescript",
        "types",
        "definitions",
        "mod",
        "sdk"
    ],
    "author": "deluca-mike",
    "homepage": "https://github.com/deluca-mike/bf6-portal-mod-types#readme",
    "bugs": {
        "url": "https://github.com/deluca-mike/bf6-portal-mod-types/issues"
    },
    "devDependencies": {
        "@types/node": "^25.0.9",
        "@typescript-eslint/eslint-plugin": "^8.53.0",
        "@typescript-eslint/parser": "^8.53.0",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.5",
        "prettier": "^3.8.0",
        "ts-morph": "^27.0.2",
        "ts-node": "^10.9.2",
        "typedoc": "^0.28.16",
        "typescript": "^5.9.3",
        "typescript-eslint": "^8.53.0"
    }
}
